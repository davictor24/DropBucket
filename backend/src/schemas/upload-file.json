{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title": "upload-file",
  "type":"object",
  "properties": {
    "name":{
      "type":"string",
      "minLength": 1
    },
    "sizeBytes":{
      "type":"integer"
    },
    "lastModified":{
      "type":"integer"
    },
    "fileUrl":{
      "type":"string",
      "minLength": 1
    }
  },
  "required":[
    "name",
    "sizeBytes",
    "lastModified",
    "fileUrl"
  ],
  "additionalProperties": false
}